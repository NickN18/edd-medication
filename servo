#include <Servo.h>

//Limit Switch connected to Pin 9

Servo myServo;

int angleSet = 0;


void setup() {
  pinMode(10, INPUT);
  myServo.attach(6);
  myServo.write(angleSet);
  
}


void loop() {

  if(myServo.read() <= 180) {
    myServo.write(angleSet + 12);
    
  } else {
    myServo.write(angleSet);
  }



}
