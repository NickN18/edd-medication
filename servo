#include <Servo.h>

//Limit Switch connected to Pin 9

Servo myServo;

int angleSet = 0;


void setup() {
  pinMode(10, INPUT);
  myServo.attach(6);
  myServo.write(angleSet);
  
}

void loop() {
  if(myServo.read() < 180) {
    angleSet += 23.5;
    myServo.write(angleSet);
    delay(2000);
  } else if(myServo.read() == 180) {
    myServo.write(0);
  } else {
    myServo.write(angleSet);
  }

}
